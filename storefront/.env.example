# Performance Analytics Configuration
# Copy this file to .env.local and configure your analytics endpoint

# Analytics endpoint for Web Vitals reporting
# Example: https://analytics.example.com/api/vitals
NEXT_PUBLIC_ANALYTICS_ENDPOINT=/api/analytics

# Enable/disable analytics in development
# Set to "true" to send analytics in development mode
NEXT_PUBLIC_DEV_ANALYTICS=false

# Bundle analyzer (set to "true" to enable)
ANALYZE=false
